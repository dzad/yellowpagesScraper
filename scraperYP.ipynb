    "def get_n_pages(content):\n",
    "    soup = BeautifulSoup(re.sub(\"\\\\\\\\\",\"\", content), 'html5lib')\n",
    "    #pag\n",
    "    pag = soup.select('div.pag > ul > li > a')\n",
    "    n_pages = 1\n",
    "    for item in pag:\n",
    "        n_pages = max(n_pages, int(item['value']))\n",
    "    return n_pages"
   ]
